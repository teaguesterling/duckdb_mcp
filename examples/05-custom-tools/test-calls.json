{
  "description": "Test calls for custom tools example - using DuckDB macros as tools",
  "calls": [
    {
      "name": "Initialize",
      "request": {
        "jsonrpc": "2.0",
        "id": 1,
        "method": "initialize",
        "params": {
          "protocolVersion": "2024-11-05",
          "capabilities": {},
          "clientInfo": {"name": "test", "version": "1.0"}
        }
      }
    },
    {
      "name": "List available tables and macros",
      "request": {
        "jsonrpc": "2.0",
        "id": 2,
        "method": "tools/call",
        "params": {
          "name": "list_tables",
          "arguments": {"include_views": true}
        }
      }
    },
    {
      "name": "Custom Tool: Search products by term",
      "request": {
        "jsonrpc": "2.0",
        "id": 3,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM search_products('laptop', NULL)"
          }
        }
      }
    },
    {
      "name": "Custom Tool: Search products by category",
      "request": {
        "jsonrpc": "2.0",
        "id": 4,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM search_products(NULL, 'Electronics')"
          }
        }
      }
    },
    {
      "name": "Custom Tool: Customer report",
      "request": {
        "jsonrpc": "2.0",
        "id": 5,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM customer_report(1)"
          }
        }
      }
    },
    {
      "name": "Custom Tool: Inventory check (low stock < 100)",
      "request": {
        "jsonrpc": "2.0",
        "id": 6,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM inventory_check(100)"
          }
        }
      }
    },
    {
      "name": "Custom Tool: Inventory check (critical < 50)",
      "request": {
        "jsonrpc": "2.0",
        "id": 7,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM inventory_check(50)"
          }
        }
      }
    },
    {
      "name": "Custom Tool: Sales summary for June 2024",
      "request": {
        "jsonrpc": "2.0",
        "id": 8,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM sales_summary('2024-06-01', '2024-06-30')"
          }
        }
      }
    },
    {
      "name": "View: Inventory status",
      "request": {
        "jsonrpc": "2.0",
        "id": 9,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM inventory_status ORDER BY stock"
          }
        }
      }
    },
    {
      "name": "View: Daily summary",
      "request": {
        "jsonrpc": "2.0",
        "id": 10,
        "method": "tools/call",
        "params": {
          "name": "query",
          "arguments": {
            "sql": "SELECT * FROM daily_summary"
          }
        }
      }
    }
  ]
}
